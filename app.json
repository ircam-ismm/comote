{
  "expo": {
    "name": "CoMote",
    "slug": "comote",
    "owner": "ircam-ismm",
    "version": "1.3.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "comote",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/82136759-44d8-4be8-912a-62f62ed6e5e6"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "fr.ircam.ismm.comote",
      "infoPlist": {
        "NSCameraUsageDescription": "Allow $(PRODUCT_NAME) to use the camera to scan QR codes",
        "NSMotionUsageDescription": "Allow $(PRODUCT_NAME) to access your device's accelerometer",
        "CFBundleAllowMixedLocalizations": true
      },
      "entitlements": {
        "com.apple.security.cs.allow-jit": true,
        "com.apple.security.device.camera": true
      },
      "supportsTablet": true,
      "buildNumber": "6"
    },
    "android": {
      "versionCode": 6,
      "softwareKeyboardLayoutMode": "pan",
      "package": "fr.ircam.ismm.comote",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "BODY_SENSORS",
        "CAMERA",
        "HIGH_SAMPLING_RATE_SENSORS"
      ]
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "82136759-44d8-4be8-912a-62f62ed6e5e6"
      }
    },
    "plugins": [
      "expo-localization"
    ],
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}
